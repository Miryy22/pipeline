    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Construire l\'application...'
            }
        }

        stage('Test') {
            steps {
                echo 'Exécuter les tests...'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Déployer l\'application...'
            }
        }
    }
}

